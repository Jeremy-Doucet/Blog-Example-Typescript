{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "inlineSourceMap": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "rootDir": "./"
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "filesGlob": [
    "**/*.ts",
    "typings/main/**/*.d.ts",
    "typings/main.d.ts",
    "!**/*.spec.ts",
    "!typings/browser/**/*.ts",
    "!typings/browser.d.ts",
    "!node_modules/**/*.ts",
    "!client/**/*.ts"
  ],
  "excludes": [
    "node_modules",
    "bower_components",
    "typings/browser/**",
    "typings/browser.d.ts",
    "client"
  ],
  "exclude": [
    "node_modules",
    "bower_components",
    "typings/browser/**",
    "typings/browser.d.ts",
    "client"
  ],
  "files": [
  "api/User/index.ts",
  "api/User/IUser.ts",
  "api/User/user.controller.ts",
  "api/User/user.model.ts",
  "app.ts",
  "auth/auth.service.ts",
  "auth/facebook/index.ts",
  "auth/facebook/passport.ts",
  "auth/google/index.ts",
  "auth/google/passport.ts",
  "auth/index.ts",
  "auth/local/controller.ts",
  "auth/local/index.ts",
  "auth/twitter/index.ts",
  "auth/twitter/passport.ts",
  "config/environment/development.ts",
  "config/environment/index.ts",
  "config/environment/production.ts",
  "config/environment/shared.ts",
  "config/environment/test.ts",
  "config/express.ts",
  "config/local.env.sample.ts",
  "config/local.env.ts",
  "config/my_typings/passport-facebook.ts",
  "index.ts",
  "routes.ts",
  "typings/main.d.ts",
  "typings/main/ambient/bcrypt/index.d.ts",
  "typings/main/ambient/body-parser/index.d.ts",
  "typings/main/ambient/compression/index.d.ts",
  "typings/main/ambient/cookie-parser/index.d.ts",
  "typings/main/ambient/errorhandler/index.d.ts",
  "typings/main/ambient/express-jwt/index.d.ts",
