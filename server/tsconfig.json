  "typings/main/ambient/jsonwebtoken/index.d.ts",
  "typings/main/ambient/lodash/index.d.ts",
  "typings/main/ambient/method-override/index.d.ts",
  "typings/main/ambient/mime/index.d.ts",
  "typings/main/ambient/mongoose/index.d.ts",
  "typings/main/ambient/morgan/index.d.ts",
  "typings/main/ambient/node/index.d.ts",
  "typings/main/ambient/passport/index.d.ts",
  "typings/main/ambient/serve-favicon/index.d.ts",
  "typings/main/ambient/serve-static/index.d.ts"
